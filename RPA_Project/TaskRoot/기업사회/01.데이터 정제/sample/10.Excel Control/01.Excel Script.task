      <EVENT KEY="73668b17-3422-4507-8042-d8f18ff20f39" EVENT_NAME="메모장 선택" EVENT_TYPE="1" CLICK_TYPE="1" POINT_CLICK_X="20" POINT_CLICK_Y="63" STOPIFERR="1" MAXIMUM_RUNTIME="10000" ALLOW_SCORE="75" MATCHING_ALGORITHM="2" IMG="" />
      <EVENT KEY="6923f2bf-c0ef-4f46-a2cc-69c2b4cff50c" EVENT_NAME="Key Typing" EVENT_TYPE="20" ISENCRYPTED="0" TYPEING_MODE="1" TYPEING_DELAY="0" USE_HW="0" TYPEING_TEXT="" PRE_CODE="using System;&#xD;&#xA;using System.IO;&#xD;&#xA;using System.Collections;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;using System.Data;&#xD;&#xA;using System.Text;&#xD;&#xA;&#xD;&#xA;public partial class CustomScript&#xD;&#xA;{&#xD;&#xA;	public void PreRun(EvPPEntry_KeyTyping EntryData)&#xD;&#xA;	{&#xD;&#xA;		StringBuilder sb = new StringBuilder();&#xD;&#xA;		&#xD;&#xA;		foreach (DataRow row in excelData.Rows)&#xD;&#xA;		{&#xD;&#xA;			object[] items = row.ItemArray;&#xD;&#xA;			foreach (object item in items)&#xD;&#xA;			{&#xD;&#xA;				sb.Append(item.ToString()).Append(&quot;\t&quot;);&#xD;&#xA;			}&#xD;&#xA;			sb.Append(&quot;\r\n&quot;);&#xD;&#xA;		}&#xD;&#xA;		&#xD;&#xA;		EntryData.TypingText = sb.ToString();&#xD;&#xA;	}&#xD;&#xA;}&#xD;&#xA;" />
