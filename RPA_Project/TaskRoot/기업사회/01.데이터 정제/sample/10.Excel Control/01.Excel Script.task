      <EVENT KEY="9d019421-c9f3-4b6b-8999-e0969e17a9e3" EVENT_NAME="Excel Script" EVENT_TYPE="80" EXCEL_SCRIPT="using System;&#xD;&#xA;using System.IO;&#xD;&#xA;using System.Collections;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;using System.Data;&#xD;&#xA;using System.Text;&#xD;&#xA;using SymationModules.ExcelControl;&#xD;&#xA;&#xD;&#xA;public partial class CustomScript&#xD;&#xA;{&#xD;&#xA;	public void Excel_Script(ExcelControlClass xl)&#xD;&#xA;	{&#xD;&#xA;		xl.SetExcelID = &quot;excel01&quot;;&#xD;&#xA;		xl.ExcelAppExecute(xl.SetExcelID, true);&#xD;&#xA;		xl.SetWorkbookID = xl.ExcelFileOpen(xl.SetExcelID, @&quot;C:\Users\KSY\Desktop\YOUTUBE_download.xlsx&quot;);&#xD;&#xA;		&#xD;&#xA;		xl.ActiveSheet(1);&#xD;&#xA;		xl.SelectCell(&quot;A3:A5&quot;);&#xD;&#xA;		&#xD;&#xA;		//xl.SelectAll(true);&#xD;&#xA;		xl.CopyToClipboard();&#xD;&#xA;		&#xD;&#xA;		excelData = xl.ClipboardToDataTable();&#xD;&#xA;		&#xD;&#xA;		xl.AddSheet(&quot;best chosen&quot;);&#xD;&#xA;		xl.ActiveSheet(1);&#xD;&#xA;		xl.SelectCell(&quot;A1&quot;);&#xD;&#xA;		&#xD;&#xA;		xl.PasteAsClipboard(false);&#xD;&#xA;		&#xD;&#xA;		xl.ExcelFileSave(xl.SetExcelID, xl.SetWorkbookID, @&quot;C:\Users\KSY\Desktop\best_chosen.xlsx&quot;);&#xD;&#xA;		xl.ExcelAppTerminate(xl.SetExcelID);&#xD;&#xA;	}&#xD;&#xA;}&#xD;&#xA;" />
