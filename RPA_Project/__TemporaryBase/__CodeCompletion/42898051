using System;
using System.IO;
using System.Collections;
using System.Collections.Generic;
using System.Data;
using System.Text;

public partial class CustomScript
{
	public void Execute_Code()
	{	
		string srcFilePath=@"C:/RPA_Generator/Report/Report Format/DailyReport_Src.docx";
		string dstFilePath = @"C:/RPA_Generator/Report/Sensor Code/" + sensorCode + "/Daily Report/Daily Report_" + dateTime + ".docx";

		dstDocxFilePath = "C:/RPA_Generator/Report/Sensor Code/" + sensorCode + "/Daily Report/Daily Report_" + dateTime + ".docx";
		
		// 기존 파일 존재하는 경우, 삭제한다
		FileInfo fileDel = new FileInfo(@dstFilePath);
		if(fileDel.Exists==true) {
  			fileDel.Delete();
		}
		
		// 파일명 변경
		System.IO.FileInfo fi = new System.IO.FileInfo(@srcFilePath);
		fi.CopyTo(@dstFilePath,false);

	}
}
