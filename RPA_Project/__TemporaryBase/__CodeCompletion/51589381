using System;
using System.IO;
using System.Collections;
using System.Collections.Generic;
using System.Data;
using System.Text;

public partial class CustomScript
{
	public void Execute_Code()
	{
		string folderPath = "C:/RPA_Generator";
		DirectoryInfo di1 = new DirectoryInfo(folderPath);
		
		// 폴더가 존재하지 않는 경우
		if(di1.Exists==false)
		{	
			di1.Create();
		}
		
		
		folderPath="C:/RPA_Generator/Report";
		DirectoryInfo di2 = new DirectoryInfo(folderPath);
		
		// 폴더가 존재하지 않는 경우
		if(di2.Exists==false)
		{	
			di2.Create();
		}
		
		folderPath="C:/RPA_Generator/Report/Monitoring CSV File";
		DirectoryInfo di3 = new DirectoryInfo(folderPath);
		
		// 폴더가 존재하지 않는 경우
		if(di3.Exists==false)
		{	
			di3.Create();
		}
		
		
		folderPath="C:/RPA_Generator/Report/Sensor Code";
		DirectoryInfo di4 = new DirectoryInfo(folderPath);
		
		// 폴더가 존재하지 않는 경우
		if(di4.Exists==false)
		{	
			di4.Create();
		}
	}
}
