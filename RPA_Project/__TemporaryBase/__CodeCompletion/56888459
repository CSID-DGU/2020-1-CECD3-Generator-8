using System;
using System.IO;
using System.Collections;
using System.Collections.Generic;
using System.Data;
using System.Text;

public partial class CustomScript
{
	public void Execute_Code()
	{
		string dateTime = System.DateTime.Now.ToString("yyyyMMddHHmmss");
		string filename = "monitoring_"+dateTime+".csv";
		string filepath="C:/RPA_Generator/Report/Monitoring CSV File/"+filename;
		
		
		string oldFilePath=@"C:/RPA_Generator/Report/Monitoring CSV File/monitoring.csv";
		string newFilePath=@filepath;
		
		
		// 파일명 변경
		System.IO.FileInfo fi = new System.IO.FileInfo(oldFilePath);
		fi.MoveTo(@newFilePath);

	}
}
