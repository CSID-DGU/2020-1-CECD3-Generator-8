using System;
using System.IO;
using System.Collections;
using System.Collections.Generic;
using System.Data;
using System.Text;

public partial class CustomScript
{
	public bool FlowItem_Script()
	{
		// 더 불러올 센서가 있으면
		if (!(String.IsNullOrEmpty(sensorCode)))
		{ 
			return _TASKRESULT=true;
		}
		
		else
		{	
			sensorIndex = 2;
			return _TASKRESULT=false;
		}
		
	}
}
