<?xml version="1.0" encoding="utf-8" standalone="yes"?><TASKSCRIPT PACING_TIME="0" EVENT_GAP_TIME="0"><TASKGROUP TYPE="TRANSACTION"><EVENTLISTS KEY="bcfc0291-c10b-45a7-a9e6-775b5038538e" NAME="Make SensorCode Dir And Copy File"><EVENT KEY="073c23da-92b3-4f10-917d-616f9d765d84" EVENT_NAME="SensorCode 디렉토리 생성" EVENT_TYPE="11" STOPIFERR="1" SCRIPT_CODE="using System;&#xD;&#xA;using System.IO;&#xD;&#xA;using System.Collections;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;using System.Data;&#xD;&#xA;using System.Text;&#xD;&#xA;&#xD;&#xA;public partial class CustomScript&#xD;&#xA;{&#xD;&#xA;	public void Execute_Code()&#xD;&#xA;	{&#xD;&#xA;		string folderPath = &quot;C:/RPA_Generator/Report/Sensor Code/&quot; + sensorCode;&#xD;&#xA;		DirectoryInfo di = new DirectoryInfo(folderPath);&#xD;&#xA;		DirectoryInfo diDaily = new DirectoryInfo(folderPath+&quot;/&quot;+&quot;Daily Report&quot;);&#xD;&#xA;		DirectoryInfo diEmerg = new DirectoryInfo(folderPath+&quot;/&quot;+&quot;Emergency Report&quot;);&#xD;&#xA;		&#xD;&#xA;		// 폴더가 존재하지 않는 경우&#xD;&#xA;		if(di.Exists==false)&#xD;&#xA;		{	&#xD;&#xA;			di.Create();&#xD;&#xA;			diDaily.Create();&#xD;&#xA;			diEmerg.Create();&#xD;&#xA;		}&#xD;&#xA;	}&#xD;&#xA;}&#xD;&#xA;" /><EVENT KEY="7c506e4a-300c-439a-9449-e23b31c2995c" EVENT_NAME="파일 복사" EVENT_TYPE="11" STOPIFERR="1" SCRIPT_CODE="using System;&#xD;&#xA;using System.IO;&#xD;&#xA;using System.Collections;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;using System.Data;&#xD;&#xA;using System.Text;&#xD;&#xA;&#xD;&#xA;public partial class CustomScript&#xD;&#xA;{&#xD;&#xA;	public void Execute_Code()&#xD;&#xA;	{	&#xD;&#xA;		string srcFilePath=@&quot;C:/RPA_Generator/Report/Report Format/DailyReport_Src.docx&quot;;&#xD;&#xA;		string dstFilePath = @&quot;C:/RPA_Generator/Report/Sensor Code/&quot; + sensorCode + &quot;/Daily Report/Daily Report_&quot; + dateTime + &quot;.docx&quot;;&#xD;&#xA;&#xD;&#xA;		dstDocxFilePath = &quot;C:/RPA_Generator/Report/Sensor Code/&quot; + sensorCode + &quot;/Daily Report/Daily Report_&quot; + dateTime + &quot;.docx&quot;;&#xD;&#xA;		&#xD;&#xA;		// 기존 파일 존재하는 경우, 삭제한다&#xD;&#xA;		FileInfo fileDel = new FileInfo(@dstFilePath);&#xD;&#xA;		if(fileDel.Exists==true) {&#xD;&#xA;  			fileDel.Delete();&#xD;&#xA;		}&#xD;&#xA;		&#xD;&#xA;		// 파일명 변경&#xD;&#xA;		System.IO.FileInfo fi = new System.IO.FileInfo(@srcFilePath);&#xD;&#xA;		fi.CopyTo(@dstFilePath,false);&#xD;&#xA;&#xD;&#xA;	}&#xD;&#xA;}&#xD;&#xA;" /><EVENT KEY="29caaf5d-d7cf-4d06-99af-1dd32b08b998" EVENT_NAME="Clipboard Write" EVENT_TYPE="42" STOPIFERR="1" CLIPBOARD_DATA="{dstDocxFilePath}" /><EVENT KEY="8792d041-154d-49c8-a78e-c09ab552069b" EVENT_NAME="바탕화면으로 이동" EVENT_TYPE="20" ISENCRYPTED="0" TYPEING_MODE="0" TYPEING_DELAY="0" USE_HW="0" TYPEING_TEXT="&lt;WIN+d&gt;" /><EVENT KEY="cfd0d4f0-2e0f-4a97-812d-08d72fae002c" EVENT_NAME="win + r" EVENT_TYPE="20" ISENCRYPTED="0" TYPEING_MODE="0" TYPEING_DELAY="0" USE_HW="0" TYPEING_TEXT="&lt;WIN+R&gt;" /><EVENT KEY="e4e1423d-eb88-4a0d-8053-515f504b0392" EVENT_NAME="붙여넣기 (생성된 파일 경로)" EVENT_TYPE="20" ISENCRYPTED="0" TYPEING_MODE="0" TYPEING_DELAY="0" USE_HW="0" TYPEING_TEXT="&lt;CTRL+v&gt;" /><EVENT KEY="2c97c815-8bf2-49d7-9f44-0d795dcf766c" EVENT_NAME="enter (생성된 파일 열기)" EVENT_TYPE="20" ISENCRYPTED="0" TYPEING_MODE="0" TYPEING_DELAY="0" USE_HW="0" TYPEING_TEXT="&lt;ENTER&gt;" /><EVENT KEY="b1eb2feb-b57d-40cf-be1f-ed6bf0e3bf94" EVENT_NAME="Wait Time" EVENT_TYPE="10" WAIT_TIME="1000" /><EVENT KEY="1918715a-0f3b-47b6-940f-e662a7e73745" EVENT_NAME="IF (더 불러올 센서가 있는 경우 실행)" EVENT_TYPE="12" SCRIPT_CODE="using System;&#xD;&#xA;using System.IO;&#xD;&#xA;using System.Collections;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;using System.Data;&#xD;&#xA;using System.Text;&#xD;&#xA;&#xD;&#xA;public partial class CustomScript&#xD;&#xA;{&#xD;&#xA;	public bool IF_Code()&#xD;&#xA;	{&#xD;&#xA;		// 더 불러올 센서가 있으면&#xD;&#xA;		if (!(String.IsNullOrEmpty(sensorCode)))&#xD;&#xA;		{ &#xD;&#xA;			return true;&#xD;&#xA;		}&#xD;&#xA;		&#xD;&#xA;		else&#xD;&#xA;		{	&#xD;&#xA;			sensorIndex = 2;&#xD;&#xA;		}&#xD;&#xA;		return false;&#xD;&#xA;	}&#xD;&#xA;}&#xD;&#xA;"></EVENT></EVENTLISTS></TASKGROUP><TASKGROUP TYPE="CUSTOM" /><TASKGROUP TYPE="EXCEPTION" /><TASKGROUP TYPE="FINALLY" /><VARIABLES><VARIABLE NAME="dateTimeReport" TYPE="string" DEFAULT="" DESCRIPTION="보고서 형식 날짜 시간" /><VARIABLE NAME="sensorCode" TYPE="string" DEFAULT="" DESCRIPTION="SensorCode 명" /><VARIABLE NAME="sensorIndex" TYPE="int" DEFAULT="" DESCRIPTION="엑셀 파일에서 순차적으로 센서에 접근" /><VARIABLE NAME="dstDocxFilePath" TYPE="string" DEFAULT="" DESCRIPTION="" /></VARIABLES></TASKSCRIPT>